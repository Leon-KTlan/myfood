{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, reactive, computed } from 'vue';\nimport { chatWithAI } from '../services/openai';\n\n// Áî®Êà∑Ê®°ÂºèÂàáÊç¢\n\nexport default {\n  __name: 'ExplorePlatform',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const userMode = ref('user');\n\n    // Ê†áÁ≠æÈ°µ\n    const userTabs = [{\n      id: 'ai',\n      name: 'AIÂä©Êâã'\n    }, {\n      id: 'restaurant',\n      name: 'Êô∫ËÉΩÈÄâÈ§ê'\n    }, {\n      id: 'community',\n      name: 'Á§æÂå∫‰∫§ÊµÅ'\n    }];\n    const activeTab = ref('ai');\n\n    // AIÂä©ÊâãÁõ∏ÂÖ≥\n    const aiMessages = ref([{\n      type: 'ai',\n      content: '‰Ω†Â•ΩÔºÅÊàëÊòØ‰Ω†ÁöÑAIÁæéÈ£üÂä©ÊâãÔºåÂèØ‰ª•Â∏Æ‰Ω†Êé®ËçêÈ§êÂéÖ„ÄÅËèúÂìÅÔºåÊàñËÄÖÂõûÁ≠î‰ªª‰ΩïÂÖ≥‰∫éÁæéÈ£üÁöÑÈóÆÈ¢ò„ÄÇÊúâ‰ªÄ‰πàÂèØ‰ª•Â∏Æ‰Ω†ÁöÑÂêóÔºü'\n    }]);\n    const aiInput = ref('');\n    const isLoading = ref(false);\n\n    // Â∞ÜÊ∂àÊÅØÂéÜÂè≤ËΩ¨Êç¢‰∏∫ API Ê†ºÂºè\n    const conversationHistory = computed(() => {\n      return aiMessages.value.filter(msg => msg.type !== 'loading').map(msg => ({\n        role: msg.type === 'user' ? 'user' : 'assistant',\n        content: msg.content\n      })).slice(1); // ÁßªÈô§Á¨¨‰∏ÄÊù°Á≥ªÁªüÊ¨¢ËøéÊ∂àÊÅØ\n    });\n    const sendAIMessage = async () => {\n      if (!aiInput.value.trim() || isLoading.value) return;\n      const userMessage = aiInput.value.trim();\n      aiInput.value = '';\n\n      // Ê∑ªÂä†Áî®Êà∑Ê∂àÊÅØ\n      aiMessages.value.push({\n        type: 'user',\n        content: userMessage\n      });\n\n      // Ê∑ªÂä†Âä†ËΩΩÊ∂àÊÅØ\n      const loadingMessageIndex = aiMessages.value.length;\n      aiMessages.value.push({\n        type: 'loading',\n        content: ''\n      });\n      isLoading.value = true;\n      try {\n        // Ë∞ÉÁî® OpenAI API\n        const response = await chatWithAI(userMessage, conversationHistory.value);\n\n        // ÁßªÈô§Âä†ËΩΩÊ∂àÊÅØÔºåÊ∑ªÂä† AI ÂõûÂ§ç\n        aiMessages.value.splice(loadingMessageIndex, 1, {\n          type: 'ai',\n          content: response\n        });\n      } catch (error) {\n        // ÁßªÈô§Âä†ËΩΩÊ∂àÊÅØÔºåÊ∑ªÂä†ÈîôËØØÊ∂àÊÅØ\n        aiMessages.value.splice(loadingMessageIndex, 1, {\n          type: 'ai',\n          content: `Êä±Ê≠âÔºåÊàëÈÅáÂà∞‰∫Ü‰∏Ä‰∫õÈóÆÈ¢òÔºö${error.message}„ÄÇËØ∑Á®çÂêéÂÜçËØïÔºåÊàñËÄÖÊ£ÄÊü•ÁΩëÁªúËøûÊé•„ÄÇ`\n        });\n        console.error('AI ÂØπËØùÈîôËØØ:', error);\n      } finally {\n        isLoading.value = false;\n\n        // ÊªöÂä®Âà∞Â∫ïÈÉ®\n        setTimeout(() => {\n          const chatContainer = document.querySelector('.overflow-y-auto');\n          if (chatContainer) {\n            chatContainer.scrollTop = chatContainer.scrollHeight;\n          }\n        }, 100);\n      }\n    };\n\n    // È§êÂéÖÊï∞ÊçÆ\n    const restaurants = ref([{\n      id: 1,\n      name: 'Â∑ùÂë≥Â∞èÂé®',\n      description: 'Ê≠£ÂÆóÂ∑ùËèúÔºåÈ∫ªËæ£È≤úÈ¶ô',\n      icon: 'üå∂Ô∏è',\n      rating: 4.8,\n      distance: 1.2,\n      foods: [{\n        id: 1,\n        name: 'È∫ªÂ©ÜË±ÜËÖê',\n        description: 'Â´©ÊªëË±ÜËÖêÈÖçÈ∫ªËæ£ËÇâÊú´',\n        price: 28,\n        icon: 'üç≤'\n      }, {\n        id: 2,\n        name: 'ÂÆ´‰øùÈ∏°‰∏Å',\n        description: 'ÁªèÂÖ∏Â∑ùËèúÔºåÈÖ∏ÁîúÂæÆËæ£',\n        price: 38,\n        icon: 'üçó'\n      }, {\n        id: 3,\n        name: 'Ê∞¥ÁÖÆÈ±º',\n        description: 'È≤úÂ´©È±ºËÇâÔºåÈ∫ªËæ£È≤úÈ¶ô',\n        price: 68,\n        icon: 'üêü'\n      }, {\n        id: 4,\n        name: 'ÂõûÈîÖËÇâ',\n        description: 'ËÇ•ËÄå‰∏çËÖªÔºåÈ¶ôËæ£‰∏ãÈ•≠',\n        price: 42,\n        icon: 'ü•©'\n      }]\n    }, {\n      id: 2,\n      name: 'Êó•ÂºèÊñôÁêÜÂ±ã',\n      description: 'Êñ∞È≤úÊó•ÊñôÔºåÁ≤æËá¥ÁæéÂë≥',\n      icon: 'üç£',\n      rating: 4.9,\n      distance: 0.8,\n      foods: [{\n        id: 5,\n        name: '‰∏âÊñáÈ±ºÂà∫Ë∫´',\n        description: 'Êñ∞È≤ú‰∏âÊñáÈ±ºÔºåÂÖ•Âè£Âç≥Âåñ',\n        price: 88,\n        icon: 'üç£'\n      }, {\n        id: 6,\n        name: 'ÂØøÂè∏ÊãºÁõò',\n        description: 'Â§öÁßçÂØøÂè∏ÁªÑÂêà',\n        price: 128,\n        icon: 'üç±'\n      }, {\n        id: 7,\n        name: 'ÊãâÈù¢',\n        description: 'ÊµìÈÉÅÊ±§Â∫ïÔºåQÂºπÈù¢Êù°',\n        price: 48,\n        icon: 'üçú'\n      }, {\n        id: 8,\n        name: 'Â§©Â¶áÁΩó',\n        description: 'ÈÖ•ËÑÜÂ§ñÁöÆÔºåÈ≤úÂ´©ÂÜÖÈáå',\n        price: 58,\n        icon: 'üç§'\n      }]\n    }, {\n      id: 3,\n      name: 'ÊÑèÂºèÈ§êÂéÖ',\n      description: 'Ê≠£ÂÆóÊÑèÂºèÈ£éÂë≥',\n      icon: 'üçù',\n      rating: 4.7,\n      distance: 1.5,\n      foods: [{\n        id: 9,\n        name: 'ÊÑèÂ§ßÂà©Èù¢',\n        description: 'ÁªèÂÖ∏Áï™ËåÑËÇâÈÖ±ÊÑèÈù¢',\n        price: 58,\n        icon: 'üçù'\n      }, {\n        id: 10,\n        name: 'Êä´Ëê®',\n        description: 'ËñÑÂ∫ïÊä´Ëê®ÔºåËäùÂ£´Êãâ‰∏ù',\n        price: 78,\n        icon: 'üçï'\n      }, {\n        id: 11,\n        name: 'ÊèêÊãâÁ±≥Ëãè',\n        description: 'ÁªèÂÖ∏ÊÑèÂºèÁîúÂìÅ',\n        price: 38,\n        icon: 'üç∞'\n      }]\n    }, {\n      id: 4,\n      name: 'Ê∏ØÂºèËå∂È§êÂéÖ',\n      description: 'Âú∞ÈÅìÊ∏ØÂºèÁæéÈ£ü',\n      icon: 'ü•ü',\n      rating: 4.6,\n      distance: 2.1,\n      foods: [{\n        id: 12,\n        name: 'ÂèâÁÉßÂåÖ',\n        description: 'È¶ôÁîúÂèâÁÉßÔºåÊùæËΩØÂåÖÂ≠ê',\n        price: 18,\n        icon: 'ü•ü'\n      }, {\n        id: 13,\n        name: 'ÁÉßÈπÖ',\n        description: 'ÁöÆËÑÜËÇâÂ´©ÔºåÈ¶ôÂë≥ÊµìÈÉÅ',\n        price: 88,\n        icon: 'ü¶Ü'\n      }, {\n        id: 14,\n        name: 'Â•∂Ëå∂',\n        description: '‰∏ùÊªëÊ∏ØÂºèÂ•∂Ëå∂',\n        price: 22,\n        icon: 'üßã'\n      }]\n    }]);\n    const selectedRestaurant = ref(null);\n    const selectRestaurant = restaurant => {\n      selectedRestaurant.value = restaurant;\n    };\n    const addToCart = food => {\n      alert(`Â∑≤Â∞Ü ${food.name} Âä†ÂÖ•Ë¥≠Áâ©ËΩ¶ÔºÅ`);\n    };\n\n    // Á§æÂå∫Áõ∏ÂÖ≥\n    const newPost = reactive({\n      content: '',\n      restaurant: ''\n    });\n    const posts = ref([{\n      id: 1,\n      user: 'ÁæéÈ£üËææ‰∫∫',\n      time: '2Â∞èÊó∂Ââç',\n      restaurant: 'Â∑ùÂë≥Â∞èÂé®',\n      content: '‰ªäÂ§©ÂéªÂêÉ‰∫ÜÈ∫ªÂ©ÜË±ÜËÖêÔºåÁúüÁöÑÂ§™È¶ô‰∫ÜÔºÅÈ∫ªËæ£ÈÄÇ‰∏≠ÔºåË±ÜËÖêÂæàÂ´©ÔºåÂº∫ÁÉàÊé®ËçêÔºÅ',\n      likes: 24,\n      liked: false,\n      comments: 5\n    }, {\n      id: 2,\n      user: 'ÂêÉË¥ßÂ∞èÁéã',\n      time: '5Â∞èÊó∂Ââç',\n      restaurant: 'Êó•ÂºèÊñôÁêÜÂ±ã',\n      content: '‰∏âÊñáÈ±ºÂà∫Ë∫´Êñ∞È≤úÂ∫¶Êª°ÂàÜÔºÅÂÖ•Âè£Âç≥ÂåñÔºå‰ª∑Ê†º‰πüÂêàÁêÜÔºå‰ºöÂÜçÊù•ÁöÑÔºÅ',\n      likes: 18,\n      liked: true,\n      comments: 3\n    }, {\n      id: 3,\n      user: 'ÁæéÈ£üÊé¢Á¥¢ËÄÖ',\n      time: '1Â§©Ââç',\n      restaurant: 'ÊÑèÂºèÈ§êÂéÖ',\n      content: 'Êä´Ëê®ÁöÑËäùÂ£´Êãâ‰∏ùÊïàÊûúÂ§™Ê£í‰∫ÜÔºÅÂ∞±ÊòØÁ≠âÁöÑÊó∂Èó¥ÊúâÁÇπÈïøÔºå‰∏çËøáÂÄºÂæóÁ≠âÂæÖ„ÄÇ',\n      likes: 31,\n      liked: false,\n      comments: 8\n    }]);\n    const createPost = () => {\n      if (!newPost.content.trim()) {\n        alert('ËØ∑ËæìÂÖ•ÂÜÖÂÆπ');\n        return;\n      }\n      posts.value.unshift({\n        id: Date.now(),\n        user: 'Êàë',\n        time: 'ÂàöÂàö',\n        restaurant: newPost.restaurant || '',\n        content: newPost.content,\n        likes: 0,\n        liked: false,\n        comments: 0\n      });\n      newPost.content = '';\n      newPost.restaurant = '';\n    };\n    const toggleLike = post => {\n      post.liked = !post.liked;\n      post.likes += post.liked ? 1 : -1;\n    };\n    const __returned__ = {\n      userMode,\n      userTabs,\n      activeTab,\n      aiMessages,\n      aiInput,\n      isLoading,\n      conversationHistory,\n      sendAIMessage,\n      restaurants,\n      selectedRestaurant,\n      selectRestaurant,\n      addToCart,\n      newPost,\n      posts,\n      createPost,\n      toggleLike,\n      ref,\n      reactive,\n      computed,\n      get chatWithAI() {\n        return chatWithAI;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","computed","chatWithAI","userMode","userTabs","id","name","activeTab","aiMessages","type","content","aiInput","isLoading","conversationHistory","value","filter","msg","map","role","slice","sendAIMessage","trim","userMessage","push","loadingMessageIndex","length","response","splice","error","message","console","setTimeout","chatContainer","document","querySelector","scrollTop","scrollHeight","restaurants","description","icon","rating","distance","foods","price","selectedRestaurant","selectRestaurant","restaurant","addToCart","food","alert","newPost","posts","user","time","likes","liked","comments","createPost","unshift","Date","now","toggleLike","post"],"sources":["/Users/leon/Desktop/Code/‰Ωú‰∏ö/webÂºÄÂèë/vue/my-food/src/components/ExplorePlatform.vue"],"sourcesContent":["<template>\n  <div class=\"min-h-screen bg-[#0B0F19] text-white font-sans\">\n    <!-- ÂØºËà™Ê†è -->\n    <nav class=\"fixed top-0 w-full z-50 border-b border-white/10 bg-[#0B0F19]/80 backdrop-blur-md\">\n      <div class=\"max-w-7xl mx-auto px-6 h-16 flex items-center justify-between\">\n        <router-link to=\"/\" class=\"text-2xl font-bold bg-gradient-to-r from-blue-400 to-purple-500 bg-clip-text text-transparent hover:opacity-80 transition\">\n          SmartCatering AI\n        </router-link>\n        \n        <!-- Áî®Êà∑/ÂïÜÂÆ∂Ê®°ÂºèÂàáÊç¢ -->\n        <div class=\"flex items-center gap-4\">\n          <div class=\"inline-flex bg-gray-800 p-1 rounded-full border border-white/10\">\n            <button \n              @click=\"userMode = 'user'\"\n              class=\"px-4 py-2 text-sm font-medium rounded-full transition-all duration-300\"\n              :class=\"userMode === 'user' \n                ? 'bg-blue-600 text-white shadow-lg shadow-blue-500/30' \n                : 'text-gray-400 hover:text-white'\">\n              Áî®Êà∑Ê®°Âºè\n            </button>\n            <button \n              @click=\"userMode = 'merchant'\"\n              class=\"px-4 py-2 text-sm font-medium rounded-full transition-all duration-300\"\n              :class=\"userMode === 'merchant' \n                ? 'bg-blue-600 text-white shadow-lg shadow-blue-500/30' \n                : 'text-gray-400 hover:text-white'\">\n              ÂïÜÂÆ∂Ê®°Âºè\n            </button>\n          </div>\n        </div>\n      </div>\n    </nav>\n\n    <!-- ‰∏ªÂÜÖÂÆπÂå∫Âüü -->\n    <div class=\"pt-16\">\n      <!-- Áî®Êà∑Ê®°Âºè -->\n      <div v-if=\"userMode === 'user'\" class=\"max-w-7xl mx-auto px-6 py-8\">\n        <!-- Ê†áÁ≠æÈ°µÂàáÊç¢ -->\n        <div class=\"flex gap-4 mb-8 border-b border-white/10\">\n          <button \n            v-for=\"tab in userTabs\" \n            :key=\"tab.id\"\n            @click=\"activeTab = tab.id\"\n            class=\"px-6 py-3 text-sm font-medium border-b-2 transition-colors\"\n            :class=\"activeTab === tab.id \n              ? 'border-blue-500 text-blue-400' \n              : 'border-transparent text-gray-400 hover:text-white'\">\n            {{ tab.name }}\n          </button>\n        </div>\n\n        <!-- AIÂä©ÊâãÊùøÂùó -->\n        <div v-if=\"activeTab === 'ai'\" class=\"space-y-6\">\n          <div class=\"bg-gradient-to-br from-blue-900/20 to-purple-900/20 rounded-2xl p-8 border border-white/10\">\n            <h2 class=\"text-3xl font-bold mb-6 flex items-center gap-3\">\n              <span class=\"text-4xl\">ü§ñ</span>\n              AI Êô∫ËÉΩÂä©Êâã\n            </h2>\n            \n            <!-- ËÅäÂ§©Âå∫Âüü -->\n            <div class=\"bg-[#0D121F] rounded-xl p-6 h-[500px] overflow-y-auto mb-4 space-y-4\">\n              <div v-for=\"(message, index) in aiMessages\" :key=\"index\" \n                   class=\"flex\" \n                   :class=\"message.type === 'user' ? 'justify-end' : 'justify-start'\">\n                <div class=\"max-w-[80%] rounded-2xl p-4\"\n                     :class=\"message.type === 'user' \n                       ? 'bg-blue-600/30 text-white' \n                       : 'bg-gray-800/50 text-gray-200'\">\n                  <div class=\"flex items-start gap-3\">\n                    <span v-if=\"message.type === 'ai'\" class=\"text-2xl\">ü§ñ</span>\n                    <div class=\"flex-1\">\n                      <p class=\"text-sm mb-1 opacity-70\">{{ message.type === 'user' ? '‰Ω†' : 'AIÂä©Êâã' }}</p>\n                      <p v-if=\"message.type !== 'loading'\" class=\"whitespace-pre-wrap\">{{ message.content }}</p>\n                      <div v-else class=\"flex items-center gap-2\">\n                        <div class=\"flex gap-1\">\n                          <div class=\"w-2 h-2 bg-blue-400 rounded-full animate-bounce\" style=\"animation-delay: 0s\"></div>\n                          <div class=\"w-2 h-2 bg-blue-400 rounded-full animate-bounce\" style=\"animation-delay: 0.2s\"></div>\n                          <div class=\"w-2 h-2 bg-blue-400 rounded-full animate-bounce\" style=\"animation-delay: 0.4s\"></div>\n                        </div>\n                        <span class=\"text-sm text-gray-400\">AI Ê≠£Âú®ÊÄùËÄÉ...</span>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            <!-- ËæìÂÖ•Ê°Ü -->\n            <div class=\"flex gap-3\">\n              <input \n                v-model=\"aiInput\"\n                @keyup.enter=\"!isLoading && sendAIMessage()\"\n                :disabled=\"isLoading\"\n                type=\"text\" \n                placeholder=\"ÈóÆÊàë‰ªª‰ΩïÂÖ≥‰∫éÁæéÈ£üÁöÑÈóÆÈ¢ò...\"\n                class=\"flex-1 bg-gray-800 border border-white/10 rounded-xl px-4 py-3 focus:outline-none focus:border-blue-500 disabled:opacity-50 disabled:cursor-not-allowed\">\n              <button \n                @click